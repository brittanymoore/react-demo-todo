	"repository": {
		"type": "git",
		"url": "git+https://github.com/brittanymoore/react-demo-todo.git"
	},
	"scripts": {
		"build": "MODE=production webpack",
		"lint": "eslint '**/*.{js,jsx}' --config ./config/lint/.eslintrc --ignore-path ./config/lint/.eslintignore",
		"start": "MODE=development webpack-dev-server",
		"start:server": "node server/server.js",
		"test": "jest --config=./config/test/jest.config.js",
		"test:e2e": "cypress run --project e2e"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"linters": {
			"**/*.{js,jsx}": [
				"prettier --write --config ./config/format/.prettierrc --ignore-path ./config/format/.prettierignore",
				"git add"
			]
		}
	},
	"dependencies": {
		"axios": "^0.18.0",
		"path-to-regexp": "^2.4.0",
		"prop-types": "^15.6.1",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-hot-loader": "^4.1.2",
		"react-router-dom": "^4.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/preset-env": "^7.1.0",
		"@babel/preset-react": "^7.0.0",
		"autoprefixer": "^9.1.5",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^23.6.0",
		"babel-loader": "^8.0.4",
		"css-loader": "^0.28.11",
		"cssnano": "^4.1.4",
		"cypress": "^3.1.2",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^5.8.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-cypress": "^2.1.2",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.11.1",
		"express": "^4.16.3",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^23.6.0",
		"json-server": "^0.14.0",
		"lint-staged": "^8.1.0",
