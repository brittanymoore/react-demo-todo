		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^5.3.0",
		"prettier": "^1.15.2",
		"react-test-renderer": "^16.4.1",
		"style-loader": "^0.21.0",
		"testdouble": "^3.8.2",
		"testdouble-jest": "^2.0.0",
		"url-loader": "^1.1.2",
		"webpack": "^4.25.1",
		"webpack-chunk-hash": "^0.6.0",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.8",
		"webpack-merge": "^4.1.2"
	}
